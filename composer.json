{
  "name": "aedart/laravel-helpers",
  "description": "Getters and Setters utility package for some of Laravel's core packages. This package make use of Laravel's native Facades, as a fallback, when no custom instances are provided.",
  "keywords": [
    "Facade",
    "helpers",
    "Laravel",
    "Getters",
    "Setters"
  ],
  "homepage": "https://github.com/aedart/laravel-helpers",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Alin Eugen Deac",
      "email": "aedart@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "5.2.*"
  },
  "require-dev": {
    "aedart/testing-laravel": "1.7.*",
    "aedart/license": "1.*",
    "aedart/license-file-manager": "~1.4",
    "predis/predis": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Aedart\\Laravel\\Helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/unit/testCases",
      "tests/integration/testCases"
    ]
  },
  "minimum-stability": "stable",
  "scripts": {
    "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
  }
}